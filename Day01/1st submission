#include <bits/stdc++.h>
using namespace std;

int main() {
    string a;
    cin>>a;
    string b;
    cin>>b;
    string c;
    c = a+b;
    cout<<c<<"\n";
    return 0;
}
2.

#include <bits/stdc++.h>
using namespace std;
int main() {
    int grade1,grade2,grade3,grade4,grade5;
    cin>>grade1;
    cin>>grade2;
    cin>>grade3;
    cin>>grade4;
    cin>>grade5;
    float avg;
    avg = (grade1+grade2+grade3+grade4)/5.0;
    cout<<avg;
    
    
    return 0;
}
