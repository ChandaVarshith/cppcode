1.
#include <iostream>
using namespace std;

int main() {
    int n, sum = 0;
    

    cin >> n;
    
    for (int i = 1; i <= n; i++) {
        sum += i;
    }
    
    cout << sum;
    
    return 0;
}
2.
#include <iostream>
using namespace std;

int main() {
    int n, factorial = 1;
    
   
    cin >> n;
    
    for (int i = 1; i <= n; i++) {
        factorial *= i;
    }
    
    cout << factorial;
    
    return 0;
}
3.
#include <iostream>
using namespace std;

int main() {
    int n;
    
   
    cin >> n;
    
  
    
    for (int i = 1; i <= 10; i++) {
        cout << n<< " x " << i << " = " << n* i << endl;
    }
    
    return 0;
}
